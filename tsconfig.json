{
    "files": [],
    "references": [
      { "path": "./tsconfig.app.json" },
      { "path": "./tsconfig.node.json" }
    ],
    "compilerOptions": {
      "baseUrl": ".",
      "jsx": "react-jsx",
      "paths": {
        "@/*": ["./src/*"]
      },
      "noImplicitAny": false,
      "noUnusedParameters": false,
      "skipLibCheck": true,
      "allowJs": true,
      "noUnusedLocals": false,
      "strictNullChecks": false
    }
  }
  
